package kr.java.chapter3;

import java.util.Scanner;

public class TTest3 {

	public static void main(String[] args) {
  
		Scanner sc = new Scanner(System.in);
    
		int num = 1;
		
		System.out.println("입력: ");
    
		int a = sc.nextInt();
    
		int[][] qwe = new int[a][a];
		
		for(int i = 0; i < a; i++) {

			for(int j = 0; j < a; j++) {
				
				
				qwe[i][j] = num % 10;
				
				System.out.print(qwe[i][j] + " ");
        
				num = num + 2;
        
			}

			System.out.println();
      
		}
		
		

	}

}
